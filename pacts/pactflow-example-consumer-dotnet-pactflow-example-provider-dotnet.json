{
  "consumer": {
    "name": "pactflow-example-consumer-dotnet"
  },
  "provider": {
    "name": "pactflow-example-provider-dotnet"
  },
  "interactions": [
    {
      "description": "A request to get products",
      "providerState": "products exist",
      "request": {
        "method": "get",
        "path": "/products"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "id": 27,
            "name": "burger",
            "type": "food"
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "A request to get a product that exists",
      "providerState": "several products exist",
      "request": {
        "method": "get",
        "path": "/product/1"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "id": 69,
          "type": "warrior",
          "name": "chhesse"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}